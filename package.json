{
  "name": "react-essential",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "type-check": "tsc --noEmit",
    "clean": "bun rimraf node_modules dist",
    "prepare": "husky",
    "build:prepare": "bun run type-check && bun run lint && bun run format && bun run build"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@tanstack/react-router": "^1.48.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "unhead": "^1.9.16"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@tanstack/router-plugin": "^1.47.0",
    "@tanstack/router-vite-plugin": "^1.47.0",
    "@types/node": "^22.4.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "husky": "^9.1.4",
    "postcss": "^8.4.41",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.1"
  }
}
